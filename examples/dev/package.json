{
  "name": "uppy-dev-example",
  "aliasify": {
    "aliases": {
      "@uppy": "../../packages/@uppy"
    }
  },
  "dependencies": {
    "@babel/core": "^7.4.4",
    "aliasify": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "babelify": "^10.0.0",
    "watchify": "^3.11.0"
  },
  "private": true,
  "scripts": {
    "watch:sandbox": "npm-run-all --parallel watch:Dashboard watch:DragDrop",
    "watch:Dashboard": "watchify -vd -t [ babelify --cwd ../../ ] -g aliasify Dashboard.js -o lib/Dashboard.js",
    "watch:DragDrop": "watchify -vd -t [ babelify --cwd ../../ ] -g aliasify DragDrop.js -o lib/DragDrop.js"
  },
  "aliasify": {
    "aliases": {
      "@uppy": "../../packages/@uppy"
    }
  }
}
